{
  "name": "react-task",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "server": "ts-node --esm server.ts",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "npx eslint .",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "cypress": "cypress open",
    "cy:run": "cypress run",
    "cy:report": "npx nyc report && npx nyc report --reporter=text-summary",
    "cy:report2": "npx nyc report --reporter",
    "cypress:coverage": "nyc instrument --compact=false src instrumented && npx cypress run --e2e && npx nyc report && npx nyc report --reporter=text-summary"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0",
    "vite-plugin-istanbul": "^4.0.1"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.4",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/cypress__code-coverage": "^3.10.0",
    "@types/express": "^4.17.17",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.29.3",
    "babel-plugin-istanbul": "^6.1.1",
    "cypress": "^12.10.0",
    "cypress-file-upload": "^5.0.8",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^7.0.4",
    "jsdom": "^21.1.1",
    "lint-staged": "^12.4.1",
    "msw": "^1.2.1",
    "nyc": "^15.1.0",
    "prettier": "^2.6.2",
    "sass": "^1.59.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vitest": "^0.29.3"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "extension": [
      ".tsx"
    ],
    "include": [
      "src/**/*.tsx"
    ],
    "exclude": [
      "src/**/*.test.tsx",
      "src/entry-server.tsx",
      "cypress/",
      "node_modules/",
      "assets/",
      "dist/",
      "main.tsx"
    ],
    "reporter": [
      "html"
    ],
    "report-dir": "coverage",
    "all": true
  }
}
